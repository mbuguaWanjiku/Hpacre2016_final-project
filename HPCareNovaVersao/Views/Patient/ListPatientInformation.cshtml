
<div class="row">
    <div class="col-md-1"></div>
    <div class="panel panel-widget col-md-10">
        <div class="progressbar-heading grids-heading" ng-controller="PatientInformationsController" ng-init="Init()">
            <h2>Patient Info of {{Name}}</h2>
        </div>

        <div class="col-md-12" ng-controller="PatientInformationsController" ng-init="Init()">
            <div class="form-title col-md-12">
                <h4>Basic Information</h4>
            </div>
            <div class="forms">
                <div class="form-horizontal">
                    <div class="form-group">
                        <label class="col-md-2 control-label">Name</label>
                        <div class="col-md-8">
                            <p class="form-control1 m-n">{{Name}}</p>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-2 control-label">Gender</label>
                        <div class="col-md-8">
                            <p class="form-control1 m-n">{{Gender}}</p>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-2 control-label">Marital Status</label>
                        <div class="col-md-8">
                            <p class="form-control1 m-n">{{MaritalStatus}}</p>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-2 control-label">Address</label>
                        <div class="col-md-8">
                            <p class="form-control1 m-n">{{Address}}</p>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-2 control-label">Email</label>
                        <div class="col-md-8">
                            <p class="form-control1 m-n">{{Email}}</p>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-2 control-label">Telephone</label>
                        <div class="col-md-8">
                            <p class="form-control1 m-n">{{Telephone}}</p>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-2 control-label">Identification</label>
                        <div class="col-md-8">
                            <p class="form-control1 m-n">{{Identification}}</p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="row">
    <div class="col-md-1"></div>
    <div class="panel panel-widget col-md-10">
        <div class="col-md-12" ng-controller="PatientInformationsController" ng-init="Init()">
            <div class="row">
                <div class="form-title">
                    <h4>Family History</h4>
                </div>
                <div class="col-md-2">
                </div>
                <div class="col-md-8">
                    <table class="table table-hover">
                        <thead>
                            <tr>
                                <th>Family History</th>
                                <th>Carrier</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr ng-repeat="FamilyHistoryCategories in PatientFamilyHistoryCategories track by $index">
                                <td> {{FamilyHistoryCategories.FamilyHistoryName}}</td>
                                <td> {{FamilyHistoryCategories.Carrier}}</td>

                            </tr>
                        </tbody>
                    </table>
                </div>
                <div class="col-md-2">
                </div>
            </div>
        </div>

    </div>
    <div class="col-md-1"></div>
</div>

<div class="row">
    <div class="col-md-1"></div>
    <div class="panel panel-widget col-md-10">
        <div class="col-md-12" ng-controller="PatientInformationsController" ng-init="Init()">
            <div class="row">
                <div class="form-title">
                    <h4>Risk Factors</h4>
                </div>
                <div class="col-md-2">
                </div>
                <div class="col-md-8">
                    <table class="table table-hover">
                        <thead>
                            <tr>
                                <th>Risk Name</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr ng-repeat="RiskFactorsCategories in PatientRiskFactorsCategories track by $index">
                                <td> {{RiskFactorsCategories.RiskFactorName}}</td>

                            </tr>
                        </tbody>
                    </table>
                </div>
                <div class="col-md-2">
                </div>
            </div>
        </div>
    </div>
    <div class="col-md-1"></div>
</div>

<div class="row">
    <div class="col-md-1"></div>
    <div class="panel panel-widget col-md-10">
        <div class="col-md-12" ng-controller="PatientInformationsController" ng-init="Init()">
            <div class="row">
                <div class="form-title">
                    <h4>Allergies</h4>
                </div>
                <div class="col-md-2">
                </div>
                <div class="col-md-8">
                    <table class="table table-hover">
                        <thead>
                            <tr>
                                <th>Name</th>
                                <th>Start Date</th>
                                <th>End Date</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr ng-repeat="AllergyCategories in PatientAllergyCategories track by $index">
                                <td> {{AllergyCategories.Allergy_Name}}</td>
                                <td> {{AllergyCategories.Allergy_start_date}}</td>
                                <td> {{AllergyCategories.Allergy_end_date}}</td>

                            </tr>
                        </tbody>
                    </table>
                </div>
                <div class="col-md-2">
                </div>
            </div>
        </div>

    </div>
    <div class="col-md-1"></div>
</div>
